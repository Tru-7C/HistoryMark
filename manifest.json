{
    "name": "HistoryMark",
    "version": "0.0.0.1",
    "manifest_version": 3,
    "description": "Mark the webpages and review them from history!",
    "icons": {
        "16": "icons/historyIcon_16.png",
        "32": "icons/historyIcon_32.png",
        "48": "icons/historyIcon_64.png",
        "128": "icons/historyIcon_128.png"
    },
    "action": {
        "default_popup": "popup/popup.html"
    },
    "permissions": [
        "history",
        "favicon"
    ]
}